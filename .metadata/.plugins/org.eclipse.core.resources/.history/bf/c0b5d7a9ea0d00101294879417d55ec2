import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class Algo1_서울_12_정민주 {
	public static void main(String[] args) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int T = Integer.parseInt(br.readLine()); //테스트케이스 개수
		int N = Integer.parseInt(br.readLine()); //색종이의 수
		int[][] paper = new int[30][30]; //도화지
		int count = 1; //카운트 변수
		for (int testCase = 1; testCase <= T; testCase++) { //테스트케이스만큼 반복
			StringTokenizer st = new StringTokenizer(br.readLine(), " ");
			int X = Integer.parseInt(st.nextToken()); //X좌표
			int Y = Integer.parseInt(st.nextToken()); //Y좌표
			for (int i = X; i < X+5; i++) { //X좌표 색종이 붙이기
				for (int j = Y; j < Y+5; j++) { //Y좌표 색종이 붙이기
				}
			}
			System.out.println("#" + testCase + " " + count);
		}// 테스트케이스 끝
	} // main 끝
} //class 끝
