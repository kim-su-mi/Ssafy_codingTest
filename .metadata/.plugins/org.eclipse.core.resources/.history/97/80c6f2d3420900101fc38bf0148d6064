package swea_asignment;
import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class swea_1952 {
	private static int minFee;
	private static int[] use;
	static int depth = 0;
	private static int[] fee;
	public static void main(String[] args) throws NumberFormatException, IOException {
		
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int T = Integer.parseInt(br.readLine());
		
		for(int testCase=1; testCase<=T; testCase++) {
			StringTokenizer st = new StringTokenizer(br.readLine());
			fee = new int[4];
			for (int i = 0; i < 4; i++) {
				fee[i] = Integer.parseInt(st.nextToken());
			}
			
			minFee = Integer.MAX_VALUE; // 최대 요금
			
			use = new int[13]; //0번 배열은 버림
			
			
			for (int i = 1; i < 12; i++) {
				st = new StringTokenizer(br.readLine());
				use[i] = Integer.parseInt(st.nextToken());
			}
			
			dfs(0,0);
			
			StringBuilder sb = new StringBuilder();
			System.out.println(sb.append("#").append(testCase).append(" ").append(" "));
			
			
		}
		
		
		
	}

	private static void dfs(int cnt, int sum) {
		if(cnt >=12) {
			minFee = Math.min(minFee,sum);
			return;
		}
		
		if(use[cnt] == 0) {
			dfs(cnt+1,sum);
		}else {
			System.out.println("day start d " + ++depth);
			dfs(cnt + 1, sum + (use[cnt] * fee[0])); // 하루 이용권 사용한 경우
			System.out.println("month start d " + ++depth);
			dfs(cnt + 1, sum + fee[1]); // 한 달 이용권 사용한 경우
			System.out.println("3month start " + ++depth);
			dfs(cnt + 3, sum + fee[2]); // 세달 이용권 사용한 경우
			System.out.println("last " + ++depth);
		}
		
	}

}
